{
  "name": "transformer-open-graph",
  "version": "0.3.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "@remark-embedder transformer for URLs that support Open Graph",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "remark",
    "remark-plugin",
    "remark-embedder",
    "transformer",
    "open-graph",
    "open graph"
  ],
  "author": "Donavon West <github@donavon.com> (https://donavon.com)",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "npm": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/donavon/transformer-open-graph"
  },
  "bugs": {
    "url": "https://github.com/donavon/transformer-open-graph/issues"
  },
  "homepage": "https://github.com/donavon/transformer-open-graph#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "setup": "npm install && npm run validate -s",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "coverage": "npm test -- --coverage",
    "typecheck": "kcd-scripts typecheck",
    "validate": "kcd-scripts validate"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "make-fetch-happen": "^10.1.1"
  },
  "devDependencies": {
    "@remark-embedder/core": "^2.0.0",
    "@types/make-fetch-happen": "^9.0.2",
    "@types/jest": "^27.4.1",
    "kcd-scripts": "^12.1.0",
    "msw": "^0.39.2",
    "remark": "^13.0.0",
    "remark-html": "^13.0.2",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "@remark-embedder/core": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "babel/camelcase": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ]
}
